<?xml version="1.0"?>
<config>
    <modules>
        <JosephLeedy_CustomFees>
            <version>0.2.0.0</version>
        </JosephLeedy_CustomFees>
    </modules>
    <global>
        <blocks>
            <custom_fees>
                <class>JosephLeedy_CustomFees_Block</class>
            </custom_fees>
        </blocks>
        <fieldsets>
            <sales_convert_quote>
                <custom_fees>
                    <to_order>*</to_order>
                </custom_fees>
            </sales_convert_quote>
            <sales_convert_order>
                <custom_fees>
                    <to_quote>*</to_quote>
                </custom_fees>
            </sales_convert_order>
        </fieldsets>
        <helpers>
            <custom_fees>
                <class>JosephLeedy_CustomFees_Helper</class>
            </custom_fees>
        </helpers>
        <models>
            <custom_fees>
                <class>JosephLeedy_CustomFees_Model</class>
            </custom_fees>
            <sales>
                <rewrite>
                    <quote_address_total_collector>JosephLeedy_CustomFees_Rewrite_Mage_Sales_Quote_Address_Total_Collector</quote_address_total_collector>
                </rewrite>
            </sales>
        </models>
        <resources>
            <custom_fees_setup>
                <setup>
                    <module>JosephLeedy_CustomFees</module>
                    <class>JosephLeedy_CustomFees_Model_Resource_Setup</class>
                </setup>
            </custom_fees_setup>
        </resources>
        <sales>
            <quote>
                <totals>
                    <custom_fees>
                        <class>custom_fees/total_quote_customFees</class>
                        <before>grand_total</before>
                        <after>tax</after>
                    </custom_fees>
                </totals>
            </quote>
            <order_invoice>
                <totals>
                    <custom_fees>
                        <class>custom_fees/total_invoice_customFees</class>
                        <before>grand_total</before>
                        <after>tax</after>
                    </custom_fees>
                </totals>
            </order_invoice>
            <order_creditmemo>
                <totals>
                    <custom_fees>
                        <class>custom_fees/total_creditmemo_customFees</class>
                        <before>grand_total</before>
                        <after>tax</after>
                    </custom_fees>
                </totals>
            </order_creditmemo>
        </sales>
    </global>
    <default>
        <sales>
            <totals_sort>
                <custom_fees>90</custom_fees>
            </totals_sort>
            <custom_order_fees>
                <custom_fees>a:1:{s:18:"_1727736651123_123";a:3:{s:4:"code";s:11:"example_fee";s:5:"title";s:11:"Example Fee";s:5:"value";s:4:"0.00";}}</custom_fees>
            </custom_order_fees>
        </sales>
    </default>
    <frontend>
        <layout>
            <updates>
                <custom_fees module="JosephLeedy_CustomFees">
                    <file>custom_fees.xml</file>
                </custom_fees>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <layout>
            <updates>
                <custom_fees module="JosephLeedy_CustomFees">
                    <file>custom_fees.xml</file>
                </custom_fees>
            </updates>
        </layout>
    </adminhtml>
</config>